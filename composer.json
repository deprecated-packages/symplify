    "scripts": {
        "check-cs": "packages/easy-coding-standard/bin/ecs check --ansi",
        "fix-cs": [
            "packages/easy-coding-standard/bin/ecs check --fix --ansi",
            "packages/easy-coding-standard/bin/ecs check-markdown README.md packages/**/README.md --fix --ansi",
            "bin/clear_readmes.sh"
        ],
        "phpstan": "vendor/bin/phpstan analyse --error-format symplify --ansi",
        "docs": [
            "packages/rule-doc-generator/bin/rule-doc-generator generate packages/coding-standard --output-file packages/coding-standard/docs/rules_overview.md",
            "packages/easy-coding-standard/bin/ecs check-markdown packages/coding-standard/docs/rules_overview.md --fix",
            "packages/rule-doc-generator/bin/rule-doc-generator generate packages/phpstan-rules --output-file packages/phpstan-rules/docs/rules_overview.md",
            "packages/easy-coding-standard/bin/ecs check-markdown packages/phpstan-rules/docs/rules_overview.md --fix"
        ],
        "changelog": [
            "packages/changelog-linker/bin/changelog-linker dump-merges --in-categories --in-packages --ansi",
            "packages/changelog-linker/bin/changelog-linker link --ansi",
            "packages/changelog-linker/bin/changelog-linker cleanup --ansi"
        ],
        "merge": "packages/monorepo-builder/bin/monorepo-builder merge --ansi",
        "propagate": "packages/monorepo-builder/bin/monorepo-builder propagate --ansi",
        "rector": "vendor/bin/rector process --dry-run --ansi",
        "validate-monorepo": "packages/monorepo-builder/bin/monorepo-builder validate --ansi",
        "release": "packages/monorepo-builder/bin/monorepo-builder release patch --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
