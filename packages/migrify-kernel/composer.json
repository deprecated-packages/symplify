{
    "name": "symplify/migrify-kernel",
    "description": "Kernel for small CLI applications",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "symfony/console": "^4.4|^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/symplify-kernel": "dev-master",
        "symplify/package-builder": "^9.0",
        "symplify/smart-file-system": "dev-master",
        "symplify/autowire-array-parameter": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.4",
        "tracy/tracy": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
