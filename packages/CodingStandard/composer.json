{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5",
        "nette/finder": "^2.4",
        "squizlabs/php_codesniffer": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.14",
        "symplify/token-runner": "^5.5",
        "slam/php-cs-fixer-extensions": "^1.17",
        "symplify/package-builder": "^5.5",
        "symplify/better-phpdoc-parser": "^5.5"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "symplify/easy-coding-standard-tester": "^5.5",
        "symplify/package-builder": "^5.5",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src",
            "Symplify\\CodingStandard\\TokenRunner\\": "packages/TokenRunner/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests",
            "Symplify\\CodingStandard\\TokenRunner\\Tests\\": "packages/TokenRunner/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
