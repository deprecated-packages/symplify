{
    "name": "symplify/skipper",
    "description": "Skip files by rule class, directory, file or fnmatch",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "symfony/config": "^4.4|^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/finder": "^4.4|^5.1",
        "symfony/filesystem": "^4.4|^5.1",
        "symplify/package-builder": "^9.0.7",
        "symplify/symplify-kernel": "^9.0.7",
        "symplify/smart-file-system": "^9.0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Skipper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Skipper\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
