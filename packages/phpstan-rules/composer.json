{
    "name": "symplify/phpstan-rules",
    "type": "phpstan-extension",
    "description": "Set of Symplify rules for PHPStan",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nikic/php-parser": "^4.11",
        "nette/utils": "^3.2",
        "phpstan/phpdoc-parser": "^0.5",
        "phpstan/phpstan": "^0.12.91",
        "symplify/astral": "^9.4.29",
        "symplify/composer-json-manipulator": "^9.4.29",
        "symplify/package-builder": "^9.4.29",
        "symplify/simple-php-doc-parser": "^9.4.29",
        "symplify/smart-file-system": "^9.4.29",
        "symplify/rule-doc-generator-contracts": "^9.4.29",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "nette/application": "^3.1",
        "nette/forms": "^3.1",
        "symplify/easy-testing": "^9.4.29",
        "symplify/phpstan-extensions": "^9.4.29",
        "symplify/rule-doc-generator": "^9.4.29",
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPStanRules\\": "src",
            "Symplify\\PHPStanRules\\CognitiveComplexity\\": "packages/cognitive-complexity/src",
            "Symplify\\PHPStanRules\\ObjectCalisthenics\\": "packages/object-calisthenics/src"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/Rules/NoFuncCallInMethodCallRule/Source/Namespaced.php"
        ],
        "psr-4": {
            "Symplify\\PHPStanRules\\Tests\\": "tests",
            "Symplify\\PHPStanRules\\ObjectCalisthenics\\Tests\\": "packages/object-calisthenics/tests",
            "Symplify\\PHPStanRules\\CognitiveComplexity\\Tests\\": "packages/cognitive-complexity/tests"
        },
        "classmap": [
            "stubs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        },
        "phpstan": {
            "includes": [
                "config/services/services.neon",
                "packages/cognitive-complexity/config/cognitive-complexity-services.neon",
                "packages/object-calisthenics/config/object-calisthenics-services.neon"
            ]
        }
    },
    "conflict": {
        "symplify/easy-coding-standard": "<9.4.29",
        "symplify/easy-testing": "<9.4.29",
        "symplify/psr4-switcher": "<9.4.29",
        "symplify/symfony-php-config": "<9.4.29",
        "symplify/php-config-printer": "<9.4.29",
        "symplify/console-color-diff": "<9.4.29",
        "symplify/autowire-array-parameter": "<9.4.29",
        "symplify/markdown-diff": "<9.4.29",
        "symplify/amnesia": "<9.4.29",
        "symplify/phpstan-extensions": "<9.4.29",
        "symplify/rule-doc-generator": "<9.4.29",
        "symplify/console-package-builder": "<9.4.29",
        "symplify/vendor-patches": "<9.4.29",
        "symplify/skipper": "<9.4.29",
        "symplify/phpunit-upgrader": "<9.4.29",
        "symplify/easy-hydrator": "<9.4.29",
        "symplify/symfony-static-dumper": "<9.4.29",
        "symplify/git-wrapper": "<9.4.29",
        "symplify/symplify-kernel": "<9.4.29",
        "symplify/monorepo-builder": "<9.4.29",
        "symplify/config-transformer": "<9.4.29",
        "symplify/easy-ci": "<9.4.29",
        "symplify/coding-standard": "<9.4.29"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
