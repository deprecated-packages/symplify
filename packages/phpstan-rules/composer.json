{
    "name": "symplify/phpstan-rules",
    "type": "phpstan-extension",
    "description": "Set of Symplify rules for PHPStan",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nikic/php-parser": "^4.14.0",
        "nette/utils": "^3.2",
        "phpstan/phpdoc-parser": "^1.6.3",
        "phpstan/phpstan": "^1.7.15",
        "symplify/astral": "^11.1",
        "symplify/composer-json-manipulator": "^11.1",
        "symplify/package-builder": "^11.1",
        "symplify/smart-file-system": "^11.1",
        "symplify/rule-doc-generator-contracts": "^11.1",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "symplify/easy-testing": "^11.1",
        "symplify/phpstan-extensions": "^11.1",
        "symplify/rule-doc-generator": "^11.1",
        "phpunit/phpunit": "^9.5",
        "myclabs/php-enum": "^1.8",
        "symfony/framework-bundle": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPStanRules\\": [
                "src",
                "packages"
            ]
        }
    },
    "autoload-dev": {
        "files": [
            "tests/Rules/NoFuncCallInMethodCallRule/Source/Namespaced.php"
        ],
        "psr-4": {
            "Symplify\\PHPStanRules\\Tests\\": [
                "tests",
                "packages-tests"
            ]
        },
        "classmap": [
            "stubs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.1-dev"
        },
        "phpstan": {
            "includes": [
                "config/services/services.neon",
                "config/packages/cognitive-complexity/cognitive-complexity-services.neon",
                "config/packages/symfony/services.neon",
                "config/packages/nette/services.neon"
            ]
        }
    },
    "conflict": {
        "symplify/easy-coding-standard": "<11.0.5",
        "symplify/easy-testing": "<11.0.5",
        "symplify/php-config-printer": "<11.0.5",
        "symplify/autowire-array-parameter": "<11.0.5",
        "symplify/phpstan-extensions": "<11.0.5",
        "symplify/rule-doc-generator": "<11.0.5",
        "symplify/vendor-patches": "<11.0.5",
        "symplify/skipper": "<11.0.5",
        "symplify/symfony-static-dumper": "<11.0.5",
        "symplify/symplify-kernel": "<11.0.5",
        "symplify/monorepo-builder": "<11.0.5",
        "symplify/config-transformer": "<11.0.5",
        "symplify/easy-ci": "<11.0.5",
        "symplify/coding-standard": "<11.0.5",
        "symplify/easy-parallel": "<11.0.5",
        "symplify/neon-config-dumper": "<11.0.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
