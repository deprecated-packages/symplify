{
    "name": "symplify/composer-json-manipulator",
    "description": "Package to load, merge and save composer.json file(s)",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.2",
        "composer/composer": "^1.10",
        "nette/utils": "^3.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/filesystem": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symplify/package-builder": "^8.2.3",
        "symplify/smart-file-system": "^8.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ComposerJsonManipulator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ComposerJsonManipulator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
