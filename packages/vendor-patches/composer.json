{
    "name": "symplify/vendor-patches",
    "description": "Generate vendor patches for packages with single command",
    "license": "MIT",
    "bin": [
        "bin/vendor-patches"
    ],
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symfony/console": "^5.3|^6.0",
        "symfony/dependency-injection": "^5.3|^6.0",
        "symfony/http-kernel": "^5.3|^6.0",
        "sebastian/diff": "^4.0",
        "cweagans/composer-patches": "^1.7",
        "symplify/composer-json-manipulator": "^9.5",
        "symplify/symplify-kernel": "^9.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\VendorPatches\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\VendorPatches\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<9.4.54",
        "symplify/easy-coding-standard": "<9.4.54",
        "symplify/phpstan-rules": "<9.4.54",
        "symplify/easy-testing": "<9.4.54",
        "symplify/psr4-switcher": "<9.4.54",
        "symplify/rule-doc-generator-contracts": "<9.4.54",
        "symplify/symfony-php-config": "<9.4.54",
        "symplify/simple-php-doc-parser": "<9.4.54",
        "symplify/php-config-printer": "<9.4.54",
        "symplify/console-color-diff": "<9.4.54",
        "symplify/autowire-array-parameter": "<9.4.54",
        "symplify/markdown-diff": "<9.4.54",
        "symplify/package-builder": "<9.4.54",
        "symplify/amnesia": "<9.4.54",
        "symplify/phpstan-extensions": "<9.4.54",
        "symplify/rule-doc-generator": "<9.4.54",
        "symplify/console-package-builder": "<9.4.54",
        "symplify/skipper": "<9.4.54",
        "symplify/smart-file-system": "<9.4.54",
        "symplify/easy-hydrator": "<9.4.54",
        "symplify/symfony-static-dumper": "<9.4.54",
        "symplify/git-wrapper": "<9.4.54",
        "symplify/monorepo-builder": "<9.4.54",
        "symplify/config-transformer": "<9.4.54",
        "symplify/easy-ci": "<9.4.54",
        "symplify/coding-standard": "<9.4.54"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
