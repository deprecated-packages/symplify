{
    "name": "symplify/easy-testing",
    "description": "Testing made easy",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "symfony/finder": "^4.4|^5.1",
        "symplify/package-builder": "^9.0.6",
        "symplify/smart-file-system": "^9.0.6",
        "symplify/symplify-kernel": "^9.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyTesting\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
