{
    "name": "symplify/easy-testing",
    "description": "Testing made easy",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.2|^8.0",
        "nette/utils": "^3.0",
        "symfony/finder": "^4.4|^5.0",
        "symplify/package-builder": "^8.2.20",
        "symplify/smart-file-system": "^8.2.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyTesting\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
