{
    "name": "symplify/statie",
    "description": "Static Site Generator",
    "license": "MIT",
    "bin": [
        "bin/statie"
    ],
    "require": {
        "php": "^7.1",
        "erusev/parsedown-extra": "^0.7.1",
        "erusev/parsedown": "^1.7",
        "latte/latte": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "J7mbo/twitter-api-php": "^1.0",
        "nette/finder": "^2.4",
        "nette/utils": "^2.5",
        "tracy/tracy": "^2.5",
        "twig/twig": "^2.5",
        "symfony/config": "^3.4|^4.1",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/event-dispatcher": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/console": "^3.4|^4.1",
        "symfony/finder": "^3.4|^4.1",
        "symfony/yaml": "^3.4|^4.1",
        "symplify/package-builder": "^5.3.10",
        "jean85/pretty-package-versions": "^1.2",
        "ocramius/package-versions": "^1.3",
        "thecodingmachine/safe": "^0.1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Statie\\": "src",
            "Symplify\\Statie\\Latte\\": "packages/Latte/src",
            "Symplify\\Statie\\Generator\\": "packages/Generator/src",
            "Symplify\\Statie\\Twig\\": "packages/Twig/src",
            "Symplify\\Statie\\Tweeter\\": "packages/Tweeter/src",
            "Symplify\\Statie\\GithubContributorsThanker\\": "packages/GithubContributorsThanker/src",
            "Symplify\\Statie\\PostHeadlineLinker\\": "packages/PostHeadlineLinker/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Statie\\Tests\\": "tests",
            "Symplify\\Statie\\Latte\\Tests\\": "packages/Latte/tests",
            "Symplify\\Statie\\Generator\\Tests\\": "packages/Generator/tests",
            "Symplify\\Statie\\Twig\\Tests\\": "packages/Twig/tests",
            "Symplify\\Statie\\Tweeter\\Tests\\": "packages/Tweeter/tests",
            "Symplify\\Statie\\PostHeadlineLinker\\Tests\\": "packages/PostHeadlineLinker/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
