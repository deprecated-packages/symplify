{
    "name": "symplify/twig-phpstan-compiler",
    "description": "Compile twig template to PHPStan-compatible PHP code to allow its static analysis",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "twig/twig": "^3.3",
        "phpstan/phpstan": "^0.12.99"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/easy-testing": "^9.4.68",
        "symplify/phpstan-rules": "^9.4.68",
        "symplify/phpstan-extensions": "^9.4.68"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\TwigPHPStanCompiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\TwigPHPStanCompiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<9.4.68",
        "symplify/composer-json-manipulator": "<9.4.68",
        "symplify/easy-coding-standard": "<9.4.68",
        "symplify/phpstan-rules": "<9.4.68",
        "symplify/easy-testing": "<9.4.68",
        "symplify/psr4-switcher": "<9.4.68",
        "symplify/rule-doc-generator-contracts": "<9.4.68",
        "symplify/symfony-php-config": "<9.4.68",
        "symplify/simple-php-doc-parser": "<9.4.68",
        "symplify/php-config-printer": "<9.4.68",
        "symplify/console-color-diff": "<9.4.68",
        "symplify/autowire-array-parameter": "<9.4.68",
        "symplify/amnesia": "<9.4.68",
        "symplify/phpstan-extensions": "<9.4.68",
        "symplify/rule-doc-generator": "<9.4.68",
        "symplify/console-package-builder": "<9.4.68",
        "symplify/vendor-patches": "<9.4.68",
        "symplify/skipper": "<9.4.68",
        "symplify/smart-file-system": "<9.4.68",
        "symplify/easy-hydrator": "<9.4.68",
        "symplify/symfony-static-dumper": "<9.4.68",
        "symplify/git-wrapper": "<9.4.68",
        "symplify/symplify-kernel": "<9.4.68",
        "symplify/monorepo-builder": "<9.4.68",
        "symplify/config-transformer": "<9.4.68",
        "symplify/easy-ci": "<9.4.68",
        "symplify/coding-standard": "<9.4.68",
        "symplify/package-builder": "<9.4.68",
        "symplify/latte-phpstan-compiler": "<9.4.68",
        "symplify/markdown-diff": "<9.4.68"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
