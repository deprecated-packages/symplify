{
    "name": "symplify/neon-config-dumper",
    "description": "Little tool to dump neon config files with services",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "nikic/php-parser": "^4.14.0",
        "symfony/console": "^6.0",
        "symplify/easy-ci": "^10.4",
        "symplify/smart-file-system": "^10.4",
        "symplify/symplify-kernel": "^10.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\NeonConfigDumper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\NeonConfigDumper\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.4-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<10.3.1",
        "symplify/composer-json-manipulator": "<10.3.1",
        "symplify/easy-coding-standard": "<10.3.1",
        "symplify/phpstan-rules": "<10.3.1",
        "symplify/rule-doc-generator-contracts": "<10.3.1",
        "symplify/php-config-printer": "<10.3.1",
        "symplify/autowire-array-parameter": "<10.3.1",
        "symplify/phpstan-extensions": "<10.3.1",
        "symplify/rule-doc-generator": "<10.3.1",
        "symplify/vendor-patches": "<10.3.1",
        "symplify/skipper": "<10.3.1",
        "symplify/smart-file-system": "<10.0.0-beta16",
        "symplify/symfony-static-dumper": "<10.3.1",
        "symplify/monorepo-builder": "<10.3.1",
        "symplify/config-transformer": "<10.3.1",
        "symplify/easy-ci": "<10.0.0-beta16",
        "symplify/coding-standard": "<10.3.1",
        "symplify/easy-parallel": "<10.3.1",
        "symplify/package-builder": "<10.3.1",
        "symplify/easy-testing": "<10.3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
