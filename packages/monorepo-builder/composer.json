{
    "name": "symplify/monorepo-builder",
    "description": "Not only Composer tools to build a Monorepo.",
    "license": "MIT",
    "bin": [
        "bin/monorepo-builder"
    ],
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "phar-io/version": "^3.1",
        "symfony/finder": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/console": "^5.3",
        "symfony/process": "^5.3",
        "symplify/package-builder": "^9.4.13",
        "symplify/composer-json-manipulator": "^9.4.13",
        "symplify/smart-file-system": "^9.4.13",
        "symplify/console-color-diff": "^9.4.13",
        "symplify/symplify-kernel": "^9.4.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\MonorepoBuilder\\": [
                "src",
                "packages"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\MonorepoBuilder\\Tests\\": [
                "tests",
                "packages-tests"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "config": {
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
