{
    "name": "symplify/psr4-switcher",
    "description": "Switch classmap to PSR-4",
    "license": "MIT",
    "bin": [
        "bin/psr4-switcher"
    ],
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "nette/robot-loader": "^3.4",
        "symfony/dependency-injection": "^5.3",
        "symfony/console": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symplify/composer-json-manipulator": "^9.5",
        "symplify/symplify-kernel": "^9.5",
        "symplify/package-builder": "^9.5",
        "symplify/smart-file-system": "^9.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Psr4Switcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Psr4Switcher\\Tests\\": "tests"
        }
    },
    "conflict": {
        "symplify/astral": "<9.4.26",
        "symplify/easy-coding-standard": "<9.4.26",
        "symplify/phpstan-rules": "<9.4.26",
        "symplify/easy-testing": "<9.4.26",
        "symplify/rule-doc-generator-contracts": "<9.4.26",
        "symplify/symfony-php-config": "<9.4.26",
        "symplify/simple-php-doc-parser": "<9.4.26",
        "symplify/php-config-printer": "<9.4.26",
        "symplify/console-color-diff": "<9.4.26",
        "symplify/autowire-array-parameter": "<9.4.26",
        "symplify/markdown-diff": "<9.4.26",
        "symplify/amnesia": "<9.4.26",
        "symplify/phpstan-extensions": "<9.4.26",
        "symplify/rule-doc-generator": "<9.4.26",
        "symplify/console-package-builder": "<9.4.26",
        "symplify/vendor-patches": "<9.4.26",
        "symplify/skipper": "<9.4.26",
        "symplify/phpunit-upgrader": "<9.4.26",
        "symplify/easy-hydrator": "<9.4.26",
        "symplify/symfony-static-dumper": "<9.4.26",
        "symplify/git-wrapper": "<9.4.26",
        "symplify/monorepo-builder": "<9.4.26",
        "symplify/config-transformer": "<9.4.26",
        "symplify/easy-ci": "<9.4.26",
        "symplify/coding-standard": "<9.4.26"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
