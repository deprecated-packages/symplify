{
    "name": "symplify/phpstan-latte-rules",
    "type": "phpstan-extension",
    "description": "Rules for static analysis of Latte templates and Latte render() methods",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "phpstan/phpstan": "^1.4.6",
        "latte/latte": "^2.10",
        "symplify/astral": "^10.2",
        "symplify/rule-doc-generator-contracts": "^10.2",
        "symplify/package-builder": "^10.2",
        "symplify/smart-file-system": "^10.2",
        "symplify/latte-phpstan-compiler": "^10.2"
    },
    "require-dev": {
        "nette/application": "^3.1",
        "nette/forms": "^3.1",
        "nette/security": "^3.1",
        "phpunit/phpunit": "^9.5",
        "symplify/phpstan-rules": "^10.2",
        "symplify/phpstan-extensions": "^10.2",
        "symplify/easy-ci": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPStanLatteRules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PHPStanLatteRules\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.2-dev"
        },
        "phpstan": {
            "includes": [
                "config/rules.neon",
                "config/services.neon"
            ]
        }
    },
    "conflict": {
        "symplify/astral": "<9.4.70",
        "symplify/composer-json-manipulator": "<10.1.0",
        "symplify/easy-coding-standard": "<10.1.0",
        "symplify/phpstan-rules": "<10.1.0",
        "symplify/easy-testing": "<10.1.0",
        "symplify/autowire-array-parameter": "<10.1.0",
        "symplify/amnesia": "<10.1.0",
        "symplify/phpstan-extensions": "<10.1.0",
        "symplify/vendor-patches": "<10.1.0",
        "symplify/skipper": "<10.1.0",
        "symplify/symfony-static-dumper": "<10.1.0",
        "symplify/git-wrapper": "<10.1.0",
        "symplify/monorepo-builder": "<10.1.0",
        "symplify/config-transformer": "<10.1.0",
        "symplify/easy-ci": "<10.1.0",
        "symplify/coding-standard": "<10.1.0",
        "symplify/latte-phpstan-compiler": "<9.4.70",
        "symplify/template-phpstan-compiler": "<10.1.0",
        "symplify/rule-doc-generator": "<10.1.0",
        "symplify/php-config-printer": "<10.1.0",
        "symplify/markdown-diff": "<10.1.0",
        "symplify/symplify-kernel": "<10.1.0",
        "symplify/easy-parallel": "<10.1.0",
        "symplify/neon-config-dumper": "<10.1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
