{
    "name": "symplify/symplify-kernel",
    "description": "Internal Kernel for Symplify packages",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/dependency-injection": "^5.1",
        "symplify/smart-file-system": "^9.0.11",
        "symplify/autowire-array-parameter": "^9.0.11",
        "symplify/composer-json-manipulator": "^9.0.11",
        "symplify/package-builder": "^9.0.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
