{
    "name": "symplify/neon-to-yaml-converter",
    "description": "Converts Neon to Yaml",
    "license": "MIT",
    "bin": [
        "bin/neon-to-yaml-converter"
    ],
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "nette/neon": "^3.2",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "symplify/symplify-kernel": "^9.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\NeonToYamlConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\NeonToYamlConverter\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
