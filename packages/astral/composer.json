{
    "name": "symplify/astral",
    "type": "phpstan-extension",
    "description": "Toolking for smart daily work with AST",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symfony/dependency-injection": "^6.0",
        "symplify/smart-file-system": "^10.3",
        "phpstan/phpstan": "^1.7.9",
        "phpstan/phpdoc-parser": "^1.5.1",
        "symfony/config": "^6.0",
        "nikic/php-parser": "^4.14.0",
        "symplify/package-builder": "^10.3",
        "symplify/symplify-kernel": "^10.3"
    },
    "require-dev": {
        "symplify/easy-testing": "^10.3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Astral\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Astral\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.3-dev"
        },
        "phpstan": {
            "includes": [
                "config/config.neon",
                "config/services.neon"
            ]
        }
    },
    "conflict": {
        "symplify/composer-json-manipulator": "<10.2.8",
        "symplify/easy-coding-standard": "<10.2.8",
        "symplify/phpstan-rules": "<10.2.8",
        "symplify/easy-testing": "<10.2.8",
        "symplify/rule-doc-generator-contracts": "<10.2.8",
        "symplify/php-config-printer": "<10.2.8",
        "symplify/phpstan-extensions": "<10.2.8",
        "symplify/rule-doc-generator": "<10.2.8",
        "symplify/vendor-patches": "<10.2.8",
        "symplify/skipper": "<10.2.8",
        "symplify/smart-file-system": "<9.4.70",
        "symplify/symfony-static-dumper": "<10.2.8",
        "symplify/symplify-kernel": "<9.4.70",
        "symplify/monorepo-builder": "<10.2.8",
        "symplify/config-transformer": "<10.2.8",
        "symplify/easy-ci": "<10.2.8",
        "symplify/coding-standard": "<10.2.8",
        "symplify/autowire-array-parameter": "<10.2.8",
        "symplify/easy-parallel": "<10.2.8",
        "symplify/neon-config-dumper": "<10.2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
