{
    "name": "symplify/rule-doc-generator",
    "description": "Documentation generator for coding standard or static analysis rules",
    "license": "MIT",
    "bin": [
        "bin/rule-doc-generator"
    ],
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": "6.1.*",
        "symfony/console": "^6.2",
        "nette/robot-loader": "^3.4",
        "symplify/symplify-kernel": "^11.1",
        "symplify/rule-doc-generator-contracts": "^11.2",
        "symplify/package-builder": "^11.2",
        "nette/utils": "^3.2",
        "sebastian/diff": "^4.0|^5.0",
        "symplify/php-config-printer": "^11.2",
        "symplify/smart-file-system": "^11.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.26",
        "friendsofphp/php-cs-fixer": "^3.12",
        "phpstan/phpstan": "^1.9.7"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\Tests\\": "tests"
        },
        "classmap": [
            "stubs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.2-dev"
        }
    },
    "conflict": {
        "symplify/phpstan-rules": "<11.1.24",
        "symplify/config-transformer": "<11.1.24",
        "symplify/coding-standard": "<11.1.24",
        "symplify/easy-ci": "<11.1.24",
        "symplify/monorepo-builder": "<11.1.24"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
