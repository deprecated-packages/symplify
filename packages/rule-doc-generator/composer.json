{
    "name": "symplify/rule-doc-generator",
    "description": "Documentation generator for coding standard or static analysis rules",
    "license": "MIT",
    "bin": [
        "bin/rule-doc-generator"
    ],
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^5.3",
        "symfony/console": "^5.3",
        "nette/robot-loader": "^3.4",
        "symplify/symplify-kernel": "^9.4.13",
        "symplify/rule-doc-generator-contracts": "^9.4.13",
        "symplify/markdown-diff": "^9.4.13",
        "symplify/package-builder": "^9.4.13",
        "nette/neon": "^3.2",
        "symplify/php-config-printer": "^9.4.13",
        "symplify/smart-file-system": "^9.4.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.90",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\Tests\\": "tests"
        },
        "classmap": [
            "stubs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
