{
    "name": "symplify/rule-doc-generator",
    "description": "Documentation generator for coding standard or static analysis rules",
    "license": "MIT",
    "bin": [
        "bin/rule-doc-generator"
    ],
    "require": {
        "php": ">=7.3",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "nette/robot-loader": "^3.2",
        "symplify/symplify-kernel": "^9.0.32",
        "symplify/markdown-diff": "^9.0.32",
        "symplify/package-builder": "^9.0.32",
        "nette/neon": "^3.2",
        "symplify/php-config-printer": "^9.0.32"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.64",
        "friendsofphp/php-cs-fixer": "^2.17.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\RuleDocGenerator\\Tests\\": "tests"
        },
        "classmap": [
            "stubs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
