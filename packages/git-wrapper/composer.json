{
    "name": "symplify/git-wrapper",
    "description": "A PHP wrapper around the Git command line utility.",
    "license": "MIT",
    "keywords": ["git", "git wrapper", "cli"],
    "require": {
        "php": ">=7.3",
        "symfony/process": "^4.4|^5.2",
        "symfony/event-dispatcher": "^4.4|^5.2",
        "nette/utils": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/smart-file-system": "^9.2.16",
        "psr/log": "^1.1",
        "ondram/ci-detector": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\GitWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\GitWrapper\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
