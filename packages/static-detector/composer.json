{
    "name": "symplify/static-detector",
    "description": "Detect static calls in your project",
    "license": "MIT",
    "bin": [
        "bin/static-detector"
    ],
    "require": {
        "php": ">=7.3",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "nikic/php-parser": "^4.10.3",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/symplify-kernel": "^9.0.8",
        "symplify/package-builder": "^9.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\StaticDetector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\StaticDetector\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
